        android:id="@+id/icon_email"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="20dp"
        android:layout_toRightOf="@id/nama"
        android:src="@drawable/icon_email"
        />

    \\icon telepon
    <ImageView
