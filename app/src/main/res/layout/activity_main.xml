        android:layout_toRightOf="@+id/foto_profile"
        android:layout_below="@+id/nama"
        android:layout_marginLeft="10dp"
        android:text="IT-Support"
        android:textSize="12dp"
        />
    \\icon email
    <ImageView
        android:layout_width="15dp"
