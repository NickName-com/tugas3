        android:layout_marginLeft="10dp"
        android:layout_marginTop="30dp"
        android:layout_toRightOf="@+id/foto_profile"
        android:text="Arif Prayogi"
        android:textColor="@color/black"
        android:textSize="18dp"
        android:textStyle="bold" />
\\\rule
