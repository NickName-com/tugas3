        android:layout_width="90dp"
        android:layout_height="100dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="25dp"
        android:src="@drawable/foto_profile"
        />
\\nama
    <TextView
        android:id="@+id/nama"
        android:layout_width="wrap_content"
