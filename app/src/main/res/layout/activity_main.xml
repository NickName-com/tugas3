\\background

    <ImageView
        android:id="@+id/bg_gambar"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/logo_bg_esa_unggul" />
\\garis atas
    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
